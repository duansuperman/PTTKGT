#include<iostream>
using namespace std;
int N;
int k[100];
int a[100];
int S;

int dem ;
void Nhap(){
    cin>>N;
    for(int i=1;i<=N;i++){
        cin>>k[i];
        S+=k[i];
    }
    S/=2;
    
}
void Try(int m){
    if(m==N+1){
        int Tong = 0;
        for(int i=1;i<=N;i++){
            if(a[i]){
                Tong+=k[i];
            }
        }
        if(Tong==S){
            for(int i=1;i<=N;i++){
                if(a[i]){
                    cout<<i<<" ";
                }
            }
            cout<<endl;
        }
        return;
    }
    for(int i=0;i<=1;i++){
        a[m]=i;
        Try(m+1);
        a[m]=0;
    }
}
int main(){
    Nhap();
    Try(1);
    return 0;
}